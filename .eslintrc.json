{
  "extends": "airbnb-base/legacy",
  "parserOptions": {
    "ecmaVersion": 8
  },
  "globals": {
    "Symbol": true,
    "Promise": true,
    "Map": true,
    "ArrayBuffer": true,
    "logger": true,
    "$VERSION": true,
    "$DEBUG": true,
    "$LocationMap": true,
    "$Notifications": true,
    "$Cache": true,
    "$Log": true,
    "ServiceWorkerGlobalScope": true,
    "expect": true,
    "afterEach": true,
    "beforeEach": true,
    "self": true,
    "clients": true,
    "describe": true,
    "it": true,
    "jest": true
  },
  "rules": {
    "global-require": 0,
    "no-prototype-builtins": 0,
    "func-names": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "class-methods-use-this": 0,
    "no-restricted-syntax": 0
  }
}
